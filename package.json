{
  "name": "diamondinsights-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "web",
    "mobile",
    "shared"
  ],
  "scripts": {
    "dev:web": "npm run dev --workspace=web",
    "build:web": "npm run build --workspace=web",
    "start:web": "npm run start --workspace=web",
    "lint:web": "npm run lint --workspace=web",
    "dev:mobile": "npm run dev --workspace=mobile",
    "build:mobile": "npm run build --workspace=mobile",
    "start:mobile": "npm run start --workspace=mobile"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20"
  }
}